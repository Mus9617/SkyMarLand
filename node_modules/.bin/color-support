#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mous/Documents/SimplonDev/BriefMysql/SkyMarLand/node_modules/color-support/node_modules:/home/mous/Documents/SimplonDev/BriefMysql/SkyMarLand/node_modules:/home/mous/Documents/SimplonDev/BriefMysql/node_modules:/home/mous/Documents/SimplonDev/node_modules:/home/mous/Documents/node_modules:/home/mous/node_modules:/home/node_modules:/node_modules:/home/mous/Documents/SimplonDev/BriefMysql/SkyMarLand/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mous/Documents/SimplonDev/BriefMysql/SkyMarLand/node_modules/color-support/node_modules:/home/mous/Documents/SimplonDev/BriefMysql/SkyMarLand/node_modules:/home/mous/Documents/SimplonDev/BriefMysql/node_modules:/home/mous/Documents/SimplonDev/node_modules:/home/mous/Documents/node_modules:/home/mous/node_modules:/home/node_modules:/node_modules:/home/mous/Documents/SimplonDev/BriefMysql/SkyMarLand/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../color-support/bin.js" "$@"
else
  exec node  "$basedir/../color-support/bin.js" "$@"
fi
